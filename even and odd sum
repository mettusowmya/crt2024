even and odd sum
#include<stdio.h>


int main() {
    int n;
    scanf("%d",&n);
    int a[n],i;
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    int esum=0,osum=0;
    for(i=0;i<n;i++){
        if(a[i]%2==0){
            esum=esum+a[i];
        }  
        
        else{
            osum=osum+a[i];
            
        }
    }
            printf("%d\n%d",osum,esum);
        
    }
    
            
           
       
        

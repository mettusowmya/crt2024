import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int p=sc.nextInt();
        int n=p/4;
        if(p>=100 && p<=200){
             System.out.println(n);
             System.out.println(n);
             System.out.println(n);
             System.out.println(n+p%4);
        }
    else{
        System.out.println("INVALID INPUT");
    }
    

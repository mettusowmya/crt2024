Ascii values
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    char n;
    scanf("%c",&n);
    if(n>=65 && n<=90)
    {
        printf("Upper",n);
    }
 else if(n>=97 && n<=122)
{
    printf("Lower",n);
}
 else if(n>=48 && n<=57)
{
    printf("Number",n);
}
    else{

    printf("Symbol",n);
    }

    return 0;
}
